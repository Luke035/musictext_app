<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.application.musictext" >
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/musictext_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SelectArtistsActivity" android:label="@string/app_name" ></activity>
        <activity android:name=".CreateUserActivity" android:label="@string/app_name"></activity>
        <activity android:name=".InsertObservationsActivity" android:label="@string/app_name"></activity>
        <activity android:name=".ShowSuggestionsActivity" android:label="@string/app_name"></activity>
    </application>

</manifest>
